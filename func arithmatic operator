#arithmetic operators
def add(a,b):
    print(a+b)
def sub(a,b):
    print(a-b)
def mul(a,b):
    print(a*b)
def div(a,b):
    print(a/b)
def floor(a,b):
    print(a//b)
def mod(a,b):
    print(a%b)
def powe(a,b):
    print(a**b)
a=int(input("Enter a number "))
b=int(input("Enter 2nd number "))
add(a,b)
sub(a,b)
mul(a,b)
div(a,b)
floor(a,b)
mod(a,b)
powe(a,b)
